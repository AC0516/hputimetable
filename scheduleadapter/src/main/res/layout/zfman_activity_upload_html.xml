<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include layout="@layout/zfman_header_upload_html"/>

        <WebView
            android:id="@+id/id_webview"
            android:background="@color/app_white"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

    <ImageView
        android:id="@+id/iv_parseBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_upload_source"
        android:layout_gravity="right|center_horizontal|bottom"
        android:layout_marginBottom="50dp"
        android:layout_marginRight="30dp"/>

    <LinearLayout
        android:id="@+id/linear_buttongroup"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom"
        android:background="@color/app_white_slight"
        android:orientation="vertical">
        <LinearLayout
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:background="@color/theme_dark"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="URP教务"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>